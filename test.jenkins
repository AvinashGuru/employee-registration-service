pipeline {
  agent any
  options {
    buildDiscarder logRotator(artifactDaysToKeepStr: '', artifactNumToKeepStr: '5', daysToKeepStr: '', numToKeepStr: '5')
  }
  stages {
    stage('Hello') {
      steps {
        bat '''
          java -version
        '''
      }
    }
    stage('clean install') {
      steps {
        bat '''
          mvn clean install -Dmaven.test.skip
        '''
      }
    }
     stage('deploy') {
      steps {
        bat '''
          npx kill-port 8080
        '''
         bat '''
          del test.out
        '''
        bat '''
          java -jar target/emp-reg-service-0.0.1-SNAPSHOT.jar >> test.out
        '''
      }
    }
  }
}
